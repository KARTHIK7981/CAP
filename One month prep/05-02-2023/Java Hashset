import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in =new Scanner(System.in);
        int p=in.nextInt();
        in.nextLine();
        HashSet<String>m=new HashSet<String>();
        for(int i=0;i<p;i++){
            String n=in.nextLine();
            m.add(n);
            System.out.println(m.size());
        }
     
    }
}
